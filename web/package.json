{
  "name": "heekkr",
  "packageManager": "yarn@3.6.2",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf heekkr/",
    "generate": "yarn clean && find ../protos/ -name \"*.proto\" | xargs protoc -I../protos/ --js_out=import_style=commonjs:. --grpc-web_out=import_style=commonjs+dts,mode=grpcwebtext:."
  },
  "dependencies": {
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2"
  }
}
